auto br0
iface br0 inet static
address 192.168.17.205
network 192.168.17.0
netmask 255.255.255.0
broadcast 192.168.17.255
gateway 192.168.17.1
bridge_ports eth0
bridge_fd 9
bridge_hello 2
bridge_maxage 12
bridge_stp off


